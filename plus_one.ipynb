    "def plusone():\n",
    "    my_str=input(\"Enter the digits:\")\n",
    "    my_list=my_str.split(',')\n",
    "    copy_list=my_list.copy()\n",
    "    my_list.sort(reverse=True)\n",
    "    grt=my_list[0]\n",
    "    index=copy_list.index(grt)\n",
    "    val=int(grt)+1\n",
    "    copy_list[index]=str(val)\n",
    "    print(copy_list)\n",
    "plusone()"
   ]
