    "def len_word():\n",
    "    my_str=input(\"Enter a string:\")\n",
    "    strip_str=my_str.strip()\n",
    "    rev_str=strip_str[::-1]\n",
    "    count=0\n",
    "    for item in rev_str:\n",
    "        if item==' ':\n",
    "            break\n",
    "        else:\n",
    "            count=count+1\n",
    "    print(\"Length of last word=\",count)\n",
    "len_word()"
